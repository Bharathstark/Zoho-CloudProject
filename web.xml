<!-- Changes made to the below tag is to make sure that web.xml adheres to Servlet 3.0 specification.  Suggested by Framework team

	** As we use Tomcat 7, Framework supports multiple web-fragment.xml which can be found in SDJSPClasses.jar.  Filters are configured in web-fragment.xml
-->
<web-app metadata-complete="true" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-fragment_3_0.xsd" id="WebAppFragment_ID1" version="3.0">


	<!--absolute-ordering>
		<name>frameworkfilters</name>
		<name>SDPFilters</name>
		<name>mickeyclientfilters</name>
	</absolute-ordering-->

	<!--frameworkfilters Starts -->
	<!-- Dont change the order of MethodFilter. This filter is used for disable vulnerable HTTP methods  -->
	<filter>
		<filter-name>MethodFilter</filter-name>
		<filter-class>com.manageengine.servicedesk.filter.MethodFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>MethodFilter</filter-name>
		<url-pattern>/*</url-pattern>
		<dispatcher>REQUEST</dispatcher>
		<dispatcher>FORWARD</dispatcher>
	</filter-mapping>
	<!-- This filter is used for disable vulnerable HTTP methods  -->

	<filter>
		<filter-name>AssociateCredential</filter-name>
		<filter-class>com.adventnet.authentication.filter.AssociateCredential</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>AssociateCredential</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>

	<!-- Filter to remove excluded request params from the request -->
	<filter>
		<filter-name>ParamFilter</filter-name>
		<filter-class>com.adventnet.filters.ParamFilter</filter-class>
		<init-param>
			<param-name>excludeParams</param-name>
			<param-value>(.*\.|^|.*|\[('|"))(c|C)lass(\.|('|")]|\[).*</param-value>
		</init-param>
	</filter>
	<filter-mapping>
		<filter-name>ParamFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>

	<!--frameworkfilters Ends -->

	<!--SDPFilters Starts -->
