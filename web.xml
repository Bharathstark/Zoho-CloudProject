		<url-pattern>/servlet/*</url-pattern>
		<url-pattern>/sdpapi/*</url-pattern>
		<url-pattern>/sdpapiv2/*</url-pattern>
		<url-pattern>/api/*</url-pattern>
		<!-- SD-71558 issue fix -->
		<url-pattern>*.json</url-pattern>
		<url-pattern>/importssl/*</url-pattern>
	</filter-mapping>

